{
  "telemetry": {
    "metrics": false,
    "diagnostics": false
  },
  "auto_install_extensions": {
    "ansible": true,
    "java": true,
    "rust": true,
    "c": true,
    "go": true,
    "nix": true,
    "python": true,
    "terraform": true
  },
  "features": {
    "copilot": false,
    "edit_prediction_provider": "none"
  },
  "auto_update": false,
  "buffer_font_family": "ComicShannsMono Nerd Font Mono",
  "buffer_font_size": 15.5,
  "hour_format": "hour24",
  "cursor_blink": true,
  "unnecessary_code_fade": 0.5,
  "notification_panel": {
    "button": false,
  },
  "agent": {
    "inline_assistant_model": {
      "provider": "zed.dev",
      "model": "claude-sonnet-4"
    },
    "default_model": {
      "provider": "zed.dev",
      "model": "claude-sonnet-4"
    },
    "provider": null,
    "enabled": true
  },
  "outline_panel": {
    "button": true,
    "dock": "right",
    "file_icons": true,
    "folder_icons": true,
  },
  "toolbar": {
    "breadcrumbs": false,
    "quick_actions": false
  },
  "tab_bar": {
    "show": true,
    "show_nav_history_buttons": false
  },
  "relative_line_numbers": true,
  "terminal": {
    "button": true,
  },
  "theme": {
    "mode": "dark",
    "dark": "Alabaster Simple",
    "light": "One Light"
  },
  "use_autoclose": true,
  "format_on_save": "off",
  "collaboration_panel": {
    "button": false
  },
  "project_panel": {
    "git_status": true,
    "button": false,
    "dock": "left",
  },
  "git_panel": {
    "dock": "right"
  },
  "git": {
    "enabled": true,
    "git_status": true
  },
  "ui_font_family": "ComicShannsMono Nerd Font Mono",
  "ui_font_size": 18,
  "vim_mode": true,
  "vim": {
    "use_system_clipboard": "always"
  },
  "scroll_beyond_last_line": "off",
  "vertical_scroll_margin": 0,
  "scrollbar": {
    "show": "never"
  },
